<ivysettings>
	<settings defaultResolver="main" checkUpToDate="false" />
	<include file="${user.home}/ivyrepository.xml" />
	<resolvers>
		<chain name="main" returnFirst="true">
			<ibiblio name="ibiblio" m2compatible="true"/>
			<ivyrep name="warwick-rep" 
				ivyroot="http://ivy-repo.warwick.ac.uk/"
				ivypattern="[module]/ivy-[revision].xml"
				artroot="http://ivy-repo.warwick.ac.uk/"
				artpattern="[module]/[artifact]-[revision].[ext]"
			/>
		</chain>
		
		<filesystem name="local">
			<ivy pattern="${ivy.default.ivy.user.dir}/local/[organisation]/[module]/[revision]/[type]s/[artifact].[ext]" />
			<artifact pattern="${ivy.default.ivy.user.dir}/local/[organisation]/[module]/[revision]/[type]s/[artifact].[ext]" />
		</filesystem>
	</resolvers>
	<modules>
		<module organisation="warwick" name=".*" resolver="warwick-rep" />
	</modules>
</ivysettings>